-content\">\n            <div class=\"modal-header\">\n              <h5 class=\"modal-title\" >Nova cirurgia</h5>\n              <button\n                type=\"button\"\n                @click=\"closeModal()\"\n                class=\"close\">\n                <span>×</span>\n              </button>\n            </div>\n            <div class=\"modal-body\">\n              <div class=\"row\">\n                <div class=\"form-group col-8\">\n                  <label for=\"\">Nome do paciente: *</label >\n                  <input type=\"text\" class=\"form-control\" v-model=\"form.nomePaciente\" required>\n                </div>\n                <div class=\"form-group col-4\">\n                   <label class=\"\" for=\"\">Convênio *</label>\n                  <select class=\"custom-select form-control\"  v-model=\"form.convenio\" id=\"\" required>\n                    <option selected>Selecione</option>\n                    <option value=\"1\">Unimed</option>\n                    <option value=\"2\">Particular</option>\n                    <option value=\"3\">HB</option>\n                    <option value=\"3\">PM</option>\n                    <option value=\"3\">Copass</option>\n                    <option value=\"3\">Saúde Caixa</option>\n                    <option value=\"3\">Vitallis</option>\n                    <option value=\"3\">Saúde Caixa</option>\n                  </select>\n                </div>\n                <div class=\"form-group col-4\">\n                  <label for=\"\">Telefone:</label>\n                  <input type=\"text\" class=\"form-control\" v-model=\"form.telefonePaciente\" >\n                </div>\n                <div class=\"form-group col-4\">\n                  <label for=\"\">Data: *</label>\n                  <input type=\"date\" class=\"form-control\" v-model=\"form.data\" required>\n                </div>\n                <div class=\"form-group col-4\">\n                  <label for=\"\">Hora: *</label>\n                  <input type=\"time\" class=\"form-control\" v-model=\"form.hora\" required>\n                </div>\n                  <div class=\"form-group col-8\">\n                  <label for=\"\">Profissional executante: *</label>\n                  <select class=\"custom-select form-control\" id=\"\" v-model=\"form.medico\" required>\n                    <option selected>Selecione</option>\n                    <option value=\"1\">Médico 01</option>\n                    <option value=\"2\">Médico 02</option>\n                    <option value=\"3\">Médico 03</option>\n                  </select>\n                </div>\n                <div class=\"form-group col-4\">\n                  <label for=\"\">Sala: *</label>\n                  <select class=\"custom-select form-control\" id=\"\" v-model=\"form.sala\" required>\n                    <option selected>Selecione</option>\n                    <option value=\"1\">Sala 01</option>\n                    <option value=\"2\">Sala 02</option>\n                    <option value=\"3\">Sala 03</option>\n                  </select>\n                </div>\n                 <div class=\"form-group col-6\">\n                  <label for=\"\">Auxiliar 1</label>\n                  <select class=\"custom-select form-control\" v-model=\"form.auxiliar\" id=\"\">\n                    <option selected>Selecione</option>\n                    <option value=\"1\">Auxiliar 01</option>\n                    <option value=\"2\">Auxiliar 02</option>\n                    <option value=\"3\">Auxiliar 03</option>\n                  </select>\n                </div>\n                <div class=\"form-group col-6\">\n                  <label for=\"\">Auxiliar 2</label>\n                  <select class=\"custom-select form-control\" v-model=\"form.auxiliar2\" id=\"\">\n                    <option selected>Selecione</option>\n                    <option value=\"1\">Auxiliar 01</option>\n                    <option value=\"2\">Auxiliar 02</option>\n                    <option value=\"3\">Auxiliar 03</option>\n                  </select>\n                </div>\n                <div class=\"form-group col-6\">\n                  <label for=\"\">Anestesista</label>\n                  <select class=\"custom-select form-control\" v-model=\"form.anestesista\" id=\"\">\n                    <option selected>Selecione</option>\n                    <option value=\"1\">Anestesista 01</option>\n                    <option value=\"2\">Anestesista 02</option>\n                    <option value=\"3\">Anestesista 03</option>\n                  </select>\n                </div>\n                <div class=\"form-group col-6\">\n                  <label for=\"\">Pediatra</label>\n                  <select class=\"custom-select form control\" v-model=\"form.pediatra\" id=\"\">\n                    <option selected>Selecione</option>\n                    <option value=\"1\">Pediatra 01</option>\n                    <option value=\"2\">Pediatra 02</option>\n                    <option value=\"3\">Pediatra 03</option>\n                  </select>\n                </div>\n                <div class=\"form-group col-12\">\n                  <label for=\"\">Descrição do procedimento: *</label>\n                  <textarea class=\"form-control\" id=\"1\" rows=\"6\" v-model=\"form.descricao\" required></textarea>\n                </div>\n\n              </div>\n            </div>\n            <div class=\"modal-footer\">\n              <button\n                type=\"button\"\n                class=\"btn btn-secondary\"\n                @click=\"closeModal()\"\n              >\n                Fechar\n               </button>\n              <button class=\"btn btn-primary\" :disabled=\"loading\">\n                <template v-if=\"loading\">\n                  <i class=\"fa fa-spin fa-spinner\"></i>\n                  Agendando...\n                </template>\n                <template v-else>\n                  Agendar\n                </