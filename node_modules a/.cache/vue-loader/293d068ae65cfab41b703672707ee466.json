"sourceRoot":"src/components/layout","sourcesContent":["<template>\n  <div class=\"w-100 d-flex justify-content-center\">\n    <button\n      class=\"btn btn-lg btn-outline-primary w-50\"\n      @click=\"showModal = true\"\n      >\n      Cadastrar Médico\n    </button>\n\n    <form @submit.prevent=\"submit()\">\n      <div\n        class=\"modal fade \"\n        :class=\"{show: showModal}\"\n        :style=\"{display: showModal ? 'block' : 'none'}\"\n      >\n        <div class=\"modal-dialog modal-lg\">\n          <div class=\"modal-content\">\n            <div class=\"modal-header\">\n              <h5 class=\"modal-title\" >Cadastrar Médico</h5>\n              <button\n                type=\"button\"\n                @click=\"closeModal()\"\n                class=\"close\">\n                <span>×</span>\n              </button>\n            </div>\n            <div class=\"modal-body\">\n              <div class=\"row\">\n                <div class=\"form-group col-8\">\n                  <label for=\"\">Nome do Médico: *</label >\n                  <input type=\"text\" class=\"form-control\" v-model=\"medico.nome\" required>\n                </div>\n\n                <div class=\"form-group col-4\">\n                  <label for=\"\">CRM:</label>\n                  <input type=\"text\" class=\"form-control phone-ddd-mask\" id=\"telefone\" v-model=\"medico.crm\" >\n                </div>\n\n\n              </div>\n            </div>\n            <div class=\"modal-footer\">\n              <button\n                type=\"button\"\n                class=\"btn btn-secondary\"\n                @click=\"closeModal()\"\n              >\n                Fechar\n               </button>\n              <button class=\"btn btn-primary\" :disabled=\"loading\">\n                <template v-if=\"loading\">\n                  <i class=\"fa fa-spin fa-spinner\"></i>\n                  Agendando...\n                </template>\n                <template v-else>\n                  Cadastrar\n                </template>\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </form>\n      <div\n      :class=\"{show: showModal}\"\n      class=\"modal-backdrop fade\"\n      :style=\"{display: showModal ? 'block' : 'none'}\"\n    />\n\n  </div>\n</template>\n\n<script>\n\n\nexport default {\n  data() {\n    return {\n      showModal: false,\n      loading: false,\n      ref: this.$firebase.firestore().collection('medicos'),\n     medico:{}\n    }\n  },\n  methods: {\n    closeModal() {\n      this.showModal = false\n    },\n     submit(){\n        this.loading = true\n        this.ref.add(this.medico).then((docRef) => {\n          this.medico.nome = ''\n          this.medico.crm = ''\n\n\n        })\n        this.closeModal(),\n        this.loading = false\n\n     }\n  }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../../assets/scss/variables\";\n.modal {\n  overflow: auto;\n\n  color: $secundary;\n}\n</style>\n\n\n"]}]}